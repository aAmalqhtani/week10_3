contract bank2{
    int bal;
    constructor() public{
        bal=1;
    }
    function getBalance() public returns(int) {
        return bal;

    }
    function withdraw(int amt) public {
        bal = bal-amt;
    }
    function deposit(int amt) public {
        bal = bal+amt;
    }
}
